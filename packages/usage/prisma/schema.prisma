generator custom_generator {
  // provider = "node ../../node_modules/prisma-generator-paginator"
  provider = "node ../generator/dist/bin"
  output   = "../src/types"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}
